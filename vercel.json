{
  "rewrites": [
    {
      "source": "/auth/sign-in",
      "destination": "https://api.lets-git-it.online/auth/sign-in"
    },
    {
      "source": "/community/:path*",
      "destination": "https://api.lets-git-it.online/community/:path*"
    },
    {
      "source": "/ranks/latest/:path*",
      "destination": "https://api.lets-git-it.online/ranks/latest/:path*"
    },
    {
      "source": "/v4/:path*",
      "destination": "https://github-contributions-api.jogruber.de/v4/:path*"
    }
  ]
}
